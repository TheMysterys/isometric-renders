{
  "schemaVersion": 1,
  "id": "isometric-renders",
  "version": "${version}",
  "name": "Isometric Renders",
  "description": "Creates high-resolution renders of game objects",
  "authors": [
    "glisco"
  ],
  "contact": {
    "issues": "https://github.com/gliscowo/isometric-renders/issues",
    "sources": "https://github.com/gliscowo/isometric-renders"
  },
  "license": "MIT",
  "icon": "assets/isometric-renders/icon.png",
  "environment": "client",
  "accessWidener": "isometricrenders.accesswidener",
  "entrypoints": {
    "client": [
      "com.glisco.isometricrenders.IsometricRenders"
    ],
    "exo": [
      "com.glisco.isometricrenders.util.IsometricChannel"
    ]
  },
  "mixins": [
    "isometric-renders.mixins.json"
  ],
  "breaks": {
    "optifabric": "*"
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": ">=0.85.0",
    "worldmesher": ">=0.4.2",
    "minecraft": ">=1.20.2",
    "owo": ">=0.11.3"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/xrwHKktV2d"
      }
